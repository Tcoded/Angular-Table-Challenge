/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "experimentalDecorators": true,
    "target": "es6",
    "lib": [
      "es2019",
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "http.proxy": "",
  "http.proxyAuthorization": null,
  "http.proxyStrictSSL": false
}
